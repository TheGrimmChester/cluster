---
apiVersion: v1
kind: Secret
metadata:
  name: cluster-secrets-user
  namespace: flux-system
stringData:
  AWS_ACCESS_KEY_ID: "{{ bootstrap_aws_key_id }}"
  AWS_SECRET_ACCESS_KEY: "{{ bootstrap_aws_secret_access_key }}"
  S3_RESTIC_URL: "{{ bootstrap_restic_s3_url }}"
  RESTIC_PASSWORD: "{{ bootstrap_restic_password }}"
